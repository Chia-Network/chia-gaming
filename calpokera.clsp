(mod (((alice_commit new_validation_hash)) conditions)
    (include *standard-cl-22*)
    (include assert.clinc)
    (include shatree.clinc)
    (compile-file calpokerb calpokerb.clsp)
    (defconst bhash (shatree calpokerb))

    (assert
        (not
            (all
                (= new_validation_hash (sha256 bhash (sha256 1 alice_commit)))
                (= (len alice_commit) 32)
            )
        )
        conditions
    )
)
