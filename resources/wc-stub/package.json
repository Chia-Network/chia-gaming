  "description": "Stub for supporting a walletconnect connection",
  "main": "dist/index.js",
  "scripts": {
    "build": "./node_modules/.bin/tsc --project .",
    "start": "node dist/index-rollup.js",
    "dev": "ts-node-dev --respawn src/index.ts",
    "test": "jest",
    "lint": "eslint src/ --ext .ts,.tsx,.js,.jsx,.mjs",
