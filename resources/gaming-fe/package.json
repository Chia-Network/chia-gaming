{
  "name": "chia-gaming-fe",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "description": "Chia Gaming Frontend and Lobby Server",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "./node_modules/.bin/tsc --project . && ./node_modules/.bin/esbuild dist/index.js --bundle --outfile=dist/index-rollup.js && ./node_modules/.bin/esbuild dist/server.js --platform=node --bundle --outfile=dist/server-rollup.cjs && ./node_modules/.bin/esbuild dist/lobby.js --platform=node --bundle --outfile=dist/lobby-rollup.cjs",
    "start": "node dist/server-rollup.cjs",
    "dev": "ts-node-dev --respawn src/server.ts",
    "test": "./node_modules/typescript/bin/tsc -p tstestconfig.json --noEmit && node --experimental-require-module node_modules/.bin/jest --useStderr",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@types/minimist": "^1.2.5",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/rx": "^4.1.4",
    "@types/socket.io-client": "^3.0.0",
    "@walletconnect/sign-client": "^2.10.0",
    "@walletconnect/utils": "^2.10.0",
    "axios": "^1.9.0",
    "bech32-buffer": "^0.2.1",
    "chia-utils": "^1.0.10",
    "clvm-lib": "^1.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "esbuild": "^0.25.5",
    "express": "^4.18.2",
    "extensionless": "^1.9.9",
    "helmet": "^7.1.0",
    "minimist": "^1.2.8",
    "node-fetch": "^3.3.2",
    "qrcode": "^1.5.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "reconnecting-websocket": "^4.4.0",
    "rxjs": "^7.8.2",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.8.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "uuid": "^11.1.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.3",
    "@babel/generator": "^7.28.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/traverse": "^7.28.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.5",
    "@types/socket.io": "^3.0.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^10.0.0",
    "babel-preset-minify": "^0.5.2",
    "core-js": "^3.45.1",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "eslint": "^9.34.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.4",
    "jest": "^30.1.3",
    "mini-css-extract-plugin": "^2.9.4",
    "prettier": "^3.1.1",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "ts-jest": "^29.4.1",
    "ts-node-dev": "^2.0.0",
    "typed-emitter": "^2.1.0",
    "typescript": "^5.9.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "moduleDirectories": [
      "node_modules",
      "node-pkg",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/scripts/testMock.js",
      "\\.(css|less)$": "<rootDir>/scripts/testMock.js"
    },
    "transform": {
      "^.+\\.ts?$": [
        "ts-jest",
        {
          "babelConfig": {
            "presets": [
              "@babel/react",
              "@babel/env"
            ],
            "plugins": [
              "@babel/plugin-transform-spread"
            ]
          }
        }
      ]
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules",
      "<rootDir>/dist",
      "<rootDir>/node-pkg",
      "/app/node_modules"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
