{
  "name": "chia-gaming-fe",
  "version": "1.0.0",
  "description": "Chia Gaming Frontend and Lobby Server",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "./node_modules/.bin/tsc --project . && ./node_modules/.bin/esbuild dist/index.js --bundle --outfile=dist/index-rollup.js && ./node_modules/.bin/esbuild dist/server.js --platform=node --bundle --outfile=dist/server-rollup.cjs && ./node_modules/.bin/esbuild dist/lobby.js --platform=node --bundle --outfile=dist/lobby-rollup.cjs",
    "start": "node dist/server-rollup.cjs",
    "dev": "ts-node-dev --respawn src/server.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/base": "5.0.0-beta.28",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "5.15.2",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/socket.io-client": "^3.0.0",
    "@walletconnect/sign-client": "^2.20.2",
    "@walletconnect/types": "^2.20.2",
    "@web3modal/standalone": "^2.4.3",
    "axios": "^1.9.0",
    "chia-utils": "^1.0.10",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "esbuild": "^0.25.5",
    "express": "^4.18.2",
    "extensionless": "^1.9.9",
    "helmet": "^7.1.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.8.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "uuid": "^11.1.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/socket.io": "^3.0.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typed-emitter": "^2.1.0",
    "typescript": "^5.3.3"
  }
}
